    "awscred": "^1.4.2",
    "husky": "^1.1.3",
    "prettier-standard": "^16.4.1",
    "proxyquire": "^2.1.0",
    "tap": "^14.10.8"
  },
  "dependencies": {
    "@hapi/bourne": "^1.3.2",
    "aws-sdk": "^2.867.0",
    "aws-signed-axios": "^1.1.0",
    "aws-xray-sdk": "^3.2.0",
    "lambda-logger-middleware": "^1.0.3",
    "lodash": "^4.17.21",
    "middy": "^0.28.4",
    "middy-autoproxyresponse": "^0.1.0",
    "pino": "^6.11.2"
  }
